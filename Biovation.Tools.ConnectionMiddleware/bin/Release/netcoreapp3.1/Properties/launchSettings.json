{
  "$schema": "http://json.schemastore.org/launchsettings.json",
  "profiles": {
    "Biovation.Tools.ConnectionMiddleware": {
      "commandName": "Project",
      //"launchBrowser": true,
      //"launchUrl": "weatherforecast",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "applicationUrl": "http://0.0.0.0:9050"
    },
    "Docker": {
      "commandName": "Docker",
      "launchBrowser": true,
      "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}/biovation/api/device",
      "publishAllPorts": true,
      "useSSL": true
    }
  }
}