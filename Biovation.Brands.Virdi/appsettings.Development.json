{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": "Debug",
    "Override": {
      "Microsoft": "Warning",
      "System": "Warning"
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:yyyy-MM-dd HH:mm:ss.fff} {Level:u3}, {ThreadId}] {Message:lj}{NewLine}{Exception}",
          "restrictedToMinimumLevel": "Verbose"
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName" ],
    "Properties": {
      "Application": "Biovation.Brands.Virdi"
    }
  },
  "MetricsOptions": {
    "Enabled": true,
    "ReportingEnabled": true,
    "DefaultContextLabel": "Biovation.Brands.Virdi"
  },
  "AppSettings": {
    "LegoServerAddress": "http://192.168.2.180:8090/Lego.Web",
    "KafkaServerAddress": "192.168.0.123:9092",
    "BiovationServerUrl": "localhost:9038",
    "ShowLiveImageInMonitoring": "True",
    "LockDevice": "False"
  }
}
