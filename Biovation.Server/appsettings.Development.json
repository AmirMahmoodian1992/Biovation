{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    //"MinimumLevel": "Verbose",
    "MinimumLevel": "Debug",
    // "MinimumLevel": "Information",
    //"MinimumLevel": "Information",
    //"MinimumLevel": "Warning",
    //"MinimumLevel": "Error",
    //"MinimumLevel": "Fatal",
    "Override": {
      "Microsoft": "Warning",
      "System": "Warning"
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:yyyy-MM-dd HH:mm:ss.fff} {Level:u3}, {ThreadId}] {Message:lj}{NewLine}{Exception}",
          "restrictedToMinimumLevel": "Verbose"
          //"outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} [{Level:u3}] {Message:lj}{NewLine}{Exception}{Properties:j}{NewLine}"
        }
      }
      //{
      //  "Name": "RollingFile",
      //  "Args": {
      //    "pathFormat": "logs/log-{Date}.txt",
      //    "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} [{Level:u3}, {ThreadId}] {Message:lj}{NewLine}{Exception}"
      //    //"outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} [{Level:u3}] {Message:lj}{NewLine}{Exception}{Properties:j}{NewLine}"
      //  }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName" ],

    "Properties": {
      "Application": "Biovation"
    }
  },
  "MetricsOptions": {
    "Enabled": true,
    "ReportingEnabled": true,
    "DefaultContextLabel": "Biovation.Server"
  },
  "AppSettings": {
    "DataQueriesPort": "9040",
    "KafkaServerAddress": "192.168.0.123:9092",
    "UseHealthCheck": "True"
  },
  "Jwt": {
    "Key": "ThisismySecretKey",
    "Issuer": "test1",
    "Audience": "test2"
  }
}
